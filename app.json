{
  "expo": {
    "name": "react-native-expo-2024",
    "slug": "react-native-expo-2024",
    "scheme": "react-native-expo-2024",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/pizzaSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9f3333"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#9f3333"
      }
    },
    "web": {
      "favicon": "./src/assets/logo2.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
